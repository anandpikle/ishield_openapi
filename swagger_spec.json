{
  "openapi": "3.0.0",
  "info": {
    "title": "ishield.ai API using OpenAPI specifications",
    "version": "0.1.0",
    "description": "This is a simple CRUD API application made with Express and documented with OpenAPI",
    "contact": {
      "name": "ishield.ai",
      "url": "https://ishield.ai",
      "email": "info@ishield.ai"
    }
  },
  "servers": [
    {
      "url": "http://localhost:3000"
    }
  ],
  "paths": {
    "/subscriptions/getOrgDetails": {
      "post": {
        "tags": [
          "org"
        ],
        "description": "Get customer organization details",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "organization object",
            "schema": {
              "$ref": "#/components/schemas/org"
            }
          },
          "500": {
            "description": "SERVER ERROR"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "org": {
        "type": "object",
        "required": "– title – author – finished",
        "properties": {
          "teamId": {
            "type": "string",
            "description": "Team/ org id of the organization entity.",
            "example": "testteam_20220219_07"
          },
          "billingId": {
            "type": "string",
            "description": "Customer identifier for payments",
            "example": "cus_LBIMhBNSNViKFR"
          },
          "startDate": {
            "type": "string",
            "format": "date",
            "description": "The date of the record creation",
            "example": "2022-02-19T16:26:56.150+00:00"
          }
        }
      }
    }
  },
  "tags": []
}
